<?xml version="1.0" ?>
<robot name="autocar2">
  <material name="orange">
    <color rgba="1.0 0.5 0.2 1" />
  </material>
  <material name="gray">
    <color rgba="0.8 0.8 0.8 1.0" />
  </material>
  <material name="yellow">
    <color rgba="1.0 1.0 0.0 1.0"/>
  </material>
  <material name="white">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>


  <link name="base_link">
    <visual name= "body">
      <geometry>
        <box size="0.1 0.2 0.05"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <material name= "white"/>
    </visual>
  </link>

  <link name="wheel1_link">
    <visual name="wheel1">
    <geometry>
      <cylinder radius="0.025" length="0.015"/>
    </geometry>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5708 0.0"/>
    <material name= "grey"/>
  </visual>
  </link>

  <link name="wheel2_link">
    <visual name="wheel2">
    <geometry>
      <cylinder radius="0.025" length="0.015"/>
    </geometry>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5708 0.0"/>
    <material name= "grey"/>
  </visual>
  </link>

  <link name="wheel3_link">
    <visual name="wheel3">
    <geometry>
      <cylinder radius="0.025" length="0.015"/>
    </geometry>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5708 0.0"/>
    <material name= "grey"/>
    </visual>
  </link>

  <link name="wheel4_link">
    <visual name="wheel4">
    <geometry>
      <cylinder radius="0.025" length="0.015"/>
    </geometry>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5708 0.0"/>
    <material name= "grey"/>
    </visual>
  </link>

  <joint name="steering_joint1" type="revolute">
    <parent link = "base_link"/>
    <child link = "wheel1_link"/>
    <origin xyz="-0.057 0.06 -0.007" rpy="0.0 0.0 0.0"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit effort='0' lower='0' upper='0' velocity='0'/>
  </joint>

  <joint name="steering_joint2" type="revolute">
    <parent link = "base_link"/>
    <child link = "wheel2_link"/>
    <origin xyz="0.057 0.06 -0.007" rpy="0.0 0.0 0.0"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit effort='0' lower='0' upper='0' velocity='0'/>
  </joint>

  <joint name="wheel_joint1" type="continuous">
    <parent link = "base_link"/>
    <child link = "wheel3_link"/>
    <origin xyz="-0.057 -0.06 -0.007" rpy="0.0 0.0 0.0"/>
    <axis xyz="1.0 0.0 0.0"/>
  </joint>

  <joint name="wheel_joint2" type="continuous">
    <parent link = "base_link"/>
    <child link = "wheel4_link"/>
    <origin xyz="0.057 -0.06 -0.007" rpy="0.0 0.0 0.0"/>
    <axis xyz="1.0 0.0 0.0"/>
  </joint>

  <joint name="imu_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="imu_link" />
    <origin xyz="0 0 0" />
  </joint>

  <link name="imu_link">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.02 -0.02 0.02" rpy="0.0 0.0 0.0"/>
      </geometry>
      <material name="orange" />
    </visual>
  </link>

  <link name="laser">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <cylinder length="0.01" radius="0.02" />
      </geometry>
      <material name="orange" />
    </visual>
  </link>
  <link name="laser_base">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.04 -0.04 0.065" rpy="0.0 0.0 0.0"/>
      </geometry>
      <material name="orange" />
    </visual>
  </link>

  <joint name="laser_joint" type="fixed">
    <parent link="base_link" />
    <child link="laser" />
    <origin xyz="0 -0.06 0.07" rpy="0.0 0.0 -1.57"/>
  </joint>
  <joint name="laser_base_joint" type="fixed">
    <parent link="base_link" />
    <child link="laser_base" />
    <origin xyz="0 -0.06 0.03" />
  </joint>
</robot>

</robot>
